{
  "name": "library",
  "version": "0.0.0",
  "engines": {
    "node": "^10"
  },
  "private": true,
  "scripts": {
    "dev": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "server": "SET DEBUG=library:* & npm run devstart",
    "client": "cd client && npm start",
    "start": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\"",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "api-query-params": "^4.15.0",
    "async": "^2.6.2",
    "compression": "^1.7.4",
    "concurrently": "^5.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.16.0",
    "express-validator": "^6.3.0",
    "helmet": "^3.21.2",
    "http-errors": "~1.6.3",
    "moment": "^2.24.0",
    "mongoose": "^5.4.13",
    "morgan": "~1.9.1",
    "validator": "^12.1.0"
  }
}
